    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'plonky3_rangecheck'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=plonky3_rangecheck",
                    "--package=plonky3_rangecheck"
                ],
                "filter": {
                    "name": "plonky3_rangecheck",
                    "kind": "bin"
                }
            },
            "args": ["--function", "goldilocks_v1", "--value", "100"],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'plonky3_rangecheck'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=plonky3_rangecheck",
                    "--package=plonky3_rangecheck"
                ],
                "filter": {
                    "name": "plonky3_rangecheck",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}